{
  "name": "y-socket.io",
  "version": "1.0.0",
  "description": "Socket IO Connector for Yjs (Inspired by y-websocket)",
  "keywords": ["Yjs", "Socket.io", "Y-Socket.io"],
  "author": "Iv√°n Topp <ivan.topp.sandoval@gmail.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"node dist/example/server.js\"",
    "start": "node ./dist/example/server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.18.1",
    "lib0": "^0.2.52",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "y-leveldb": "^0.1.1",
    "y-protocols": "^1.0.5"    
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "yjs": "^13.5.41"
  },
  "peerDependencies": {
    "yjs": "^13.5.6"
  }
}
